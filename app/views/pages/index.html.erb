<h1>Dashboard</h1>

<% @books.each do |book| %>
  <div class='panel'>
    <h3><%= book.title %></h3>
    <p><%= book.author %></p>
    <% if book.checked_out? %>
      <span class='label radius warning'>This book is checked out to <%= book.checked_out_book.friend.full_name %>.</span>
    <% end %>
  </div>
<% end %>
